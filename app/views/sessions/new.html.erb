<h4>Login Here</h4>
<div class = "card-content">
<br />
    <%= form_with url: sessions_path, local: true do |f| %>
    <div class="control-group">
        <%= f.label :username %>
        <div class="controls">
            <%= f.text_field :username %>
        </div>
    </div>

    <div class="control-group">
        <%= f.label :password %>
        <div class="controls">
            <%= f.password_field :password %>
        </div>
    </div>
    
    <div class="actions"><%= f.submit "Log In" %></div>
<% end %>
</div>